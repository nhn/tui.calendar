    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "build": "lerna run build",
    "build:calendar": "lerna run --scope @toast-ui/calendar build",
    "build:react": "lerna run --scope @toast-ui/react-calendar build",
    "build:vue": "lerna run --scope @toast-ui/vue-calendar build",
    "build:date": "lerna run --scope @toast-ui/date build",
    "build:calendar-storybook": "lerna run --scope @toast-ui/calendar storybook:build",
    "serve:storybook": "http-server ./apps/calendar/storybook-static",
    "install:date": "cd libs/date && npm install",
    "setup:date": "npm run install:date && npm run build:date",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:playwright": "playwright test --browser=chromium",
    "test:playwright:ci": "playwright test --browser=all",
    "test:playwright:inspect": "playwright test --browser=chromium --debug",
    "test:playwright:watch": "chokidar apps/calendar/playwright/**/*.spec.ts -c 'npm run test:playwright'",
    "prepare": "husky install && ts-patch install -s"
  },
  "workspaces": [
    "apps/calendar",
    "libs/*"
  ],
  "dependencies": {
    "preact": "^10.6.4",
    "preact-render-to-string": "^5.1.19",
    "immer": "^9.0.7",
    "isomorphic-dompurify": "^0.17.0",
    "tui-code-snippet": "^2.3.3"
  }
}

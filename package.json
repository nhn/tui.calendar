  "scripts": {
    "test": "karma start --no-single-run",
    "test:ne": "KARMA_SERVER=ne karma start",
    "test:types": "tsc --project test/types",
    "bundle": "webpack --mode development && NODE_ENV=production webpack --mode production && node tsBannerGenerator.js",
    "serve": "DEV_SERVER=true webpack-dev-server --mode development",
    "cpy-dist2doc": "mkdir -p doc/dist && cp -f -r dist doc",
    "release-note": "tuie",
    "eslint": "eslint ./src/ts ./test-ts --ext ts --ext tsx",
    "tslint": "tslint index.d.ts",
    "doc:dev": "tuidoc --dev",
    "doc": "tuidoc",
    "build:dev": "webpack --mode development",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build:dev:es6": "TARGET=es6 webpack --mode development",
    "build:prod:es6": "TARGET=es6 NODE_ENV=production webpack --mode production",
    "build:esm": "ttsc -p tsconfig.esm.json && prettier dist/esm/**/*.js --write",
    "build": "rm -rf dist && npm run build:dev && npm run build:prod && npm run build:dev:es6 && npm run build:prod:es6 && npm run build:esm",
    "analyze": "NODE_ENV=production webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json ./dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
